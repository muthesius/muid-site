<section class="slides">
	{{#each model.images as |medium|}}
		<img src="{{attachment-url model medium "thumb-large"}}" alt="">
	{{/each}}
</section>
<h1>{{model.title}}</h1>
<h2>{{model.authors}}</h2>
<section class="abstract">{{model.abstract}}</section>
{{#if model.description}}
	<section class="description">{{model.description}}</section>
{{/if}}